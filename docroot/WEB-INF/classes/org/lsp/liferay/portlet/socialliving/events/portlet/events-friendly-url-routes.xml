<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">

<routes>
	<route>
		<pattern></pattern>
		<implicit-parameter name="jspPage">/events/view.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/entry/{eventEntryId:\d+}</pattern>
		<implicit-parameter name="jspPage">/common/view_entry.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/user-registrations/{format}</pattern>
		<generated-parameter name="resource">{format}</generated-parameter>
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
	</route>
</routes>